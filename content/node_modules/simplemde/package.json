    "gulp-uglify": "*",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-buffer": "*"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "spell-checker": "./node_modules/codemirror-spell-checker/src/js/spell-checker.js",
    "typo": "./node_modules/codemirror-spell-checker/src/js/typo.js"
  },
  "browserify-shim": {
    "spell-checker": {
      "depends": [
        "typo:Typo",
        "codemirror:CodeMirror"
      ]
    },
    "typo": {
      "exports": "Typo"
    }
